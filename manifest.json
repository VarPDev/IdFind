{
	"manifest_version": 2,
	"name": "Id track",
	"version": "0.1",
  
	"content_scripts": [
	  {
		"matches": [
		  "<all_urls>"
		],
		"css": ["styles.css"],
		"js": ["jquery-3.2.1.js", "content.js"]
	  }
	],
	
	"browser_action": {
	  "default_icon": "icon.png",
	  "default_popup": "popup.html"
	  },
	 
	"permissions": ["tabs", "<all_urls>"],
	 
	"background": {
	  "scripts": ["background.js"]
	}
	
}

